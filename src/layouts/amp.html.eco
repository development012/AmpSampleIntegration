      /* Accordian css */

      
.half {
  float: left;
  width: 90%;
  padding: 0 1em;
}
/* Acordeon styles */
.tab {
  position: relative;
  margin-bottom: 1px;
  width: 105%;
  color: #000;
  overflow: hidden;
}
input {
  position: absolute;
  opacity: 0;
  z-index: -1;
}
.plusIcon {
    position: relative;
    display: block;
    padding: 0 0 0 1em;
    background: #fff;
    border-top: 1px solid #ccc;
    font-weight: bold;
    line-height: 5;
    cursor: pointer;
    font-size: 20px
}

.tab-content {
  max-height: 0;
  overflow: hidden;
  background: #fff;
  transition: max-height .35s;
}

.tab-content p {
   font-family: 'Roboto', sans-serif;
    line-height: 28px;
    font-size: 18px;
}
/* :checked */
input:checked ~ .tab-content {
  max-height: 10em;
}
/* Icon */
.plusIcon::after {
    position: absolute;
    right: 0;
    top: 0;
    font-size: 33px;
    display: block;
    width: 3em;
    height: 3em;
    line-height: 3;
    text-align: center;
    transition: all .35s;
} 
input[type=checkbox] + label::after {
  content: "+";
}
input[type=radio] + label::after {
  content: "\25BC";
}
input[type=checkbox]:checked + label::after {
  transform: rotate(315deg);
}
input[type=radio]:checked + label::after {
  transform: rotateX(180deg);
}
